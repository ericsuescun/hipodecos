<p id="notice"><%= notice %></p>

<h1>Autos</h1>

<table>
  <thead>
    <tr>
      <th>Diagcode</th>
      <th>User</th>
      <th>Admin</th>
      <th>Title</th>
      <th>Body</th>
      <th>Param1</th>
      <th>Param2</th>
      <th>Param3</th>
      <th>Param4</th>
      <th>Param5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @autos.each do |auto| %>
      <tr>
        <td><%= auto.diagcode %></td>
        <td><%= auto.user_id %></td>
        <td><%= auto.admin_id %></td>
        <td><%= auto.title %></td>
        <td><%= auto.body %></td>
        <td><%= auto.param1 %></td>
        <td><%= auto.param2 %></td>
        <td><%= auto.param3 %></td>
        <td><%= auto.param4 %></td>
        <td><%= auto.param5 %></td>
        <td><%= link_to 'Show', auto %></td>
        <td><%= link_to 'Edit', edit_auto_path(auto) %></td>
        <td><%= link_to 'Destroy', auto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auto', new_auto_path %>
