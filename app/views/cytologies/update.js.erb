$(document).on('page:load', refresh());

function refresh() {

	$('div.cytologies').remove();
	$('div.cytology').append('<%= j render "informs/cytologies", inform: @inform, edit_cytology_form: nil, edit_objection_id: nil %>');

	
	
	setTimeout(function() {
		$('div.alert-growl').remove();
	}, 3000)
}