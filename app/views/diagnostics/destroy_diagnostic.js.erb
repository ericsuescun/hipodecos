$(document).on('page:load', refresh());

function refresh() {

	$('div.diagnostics-out').remove();
	$('div.diagnostics-in').append('<%= j render "diagnostics/diagnostics", inform: @inform, edit_diagnostic: nil, edit_objection_id: nil %>');

	
	
	setTimeout(function() {
		$('div.alert-growl').remove();
	}, 3000)
}