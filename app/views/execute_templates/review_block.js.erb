<% if @inform.errors.empty? %>
	$(document).on('page:load', refresh2());
	function refresh2() {

	  $('div.blocks').remove();
	  $('div.block').append('<%= j render "informs/blocks", inform: @inform, blocks: @blocks, samplesc: @samplesc, edit_objection_id: @objection.id %>');

	  
	  $('div.fast-bar').append('<%= j render "ok-message", message: "revisado" %>');
	  setTimeout(function() {
	  	$('div.alert-growl').remove();
	  }, 3000)
	}
<% end %>