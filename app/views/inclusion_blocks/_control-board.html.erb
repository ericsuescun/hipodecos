<div class="inclusion-blocks">
  <div class="container-fluid">
    <div class="row">
      <div class="col-12 col-lg-12 col-xl-12">
        
          <div class="row">
            <div class="col-12 col-lg-2 col-xl-2">
              <% blocks.each_with_index do |block2, n| %>
                <% if n < (blocks.size / 3) + 1 %>
                  <%= render partial: "inclusion_blocks/block-table", locals: { block2: block2 } %>
                <% end %>
              <% end %>  
            </div>
            <div class="col-12 col-lg-2 col-xl-2">
              <% blocks.each_with_index do |block2, n| %>
                <% if n > (blocks.size / 3) + 1  && n < 2 * (blocks.size / 3) + 1 %>
                  <%= render partial: "inclusion_blocks/block-table", locals: { block2: block2 } %>
                <% end %>
              <% end %>
            </div>
            <div class="col-12 col-lg-2 col-xl-2">
              <% blocks.each_with_index do |block2, n| %>
                <% if n > 2 * (blocks.size / 3) %>
                  <%= render partial: "inclusion_blocks/block-table", locals: { block2: block2 } %>
                <% end %>
              <% end %>
            </div>
          </div>

      </div>
    </div>
  </div>
</div>