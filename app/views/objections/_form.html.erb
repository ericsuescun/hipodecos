<%= form_for [objectionable, objectionable.objections.build] do |form| %>
  
  <div class="form-group">
    <%= form.label "Título de NC" %>
    <%= form.select(:name, options_for_select([['Se recibe muestra que no es', 'Se recibe muestra que no es'], ['Cambio de CUPS','Cambio de CUPS'], ['Error en descripción macroscópica', 'Error en descripción macroscópica'], ['Descripción macro incompleta', 'Descripción macro incompleta'], ['Bloque mal marcado', 'Bloque mal marcado'], ['Bloque crudo', 'Bloque crudo'], ['Bloque mal incluido', 'Bloque mal incluido']]), { include_blank: true }, class: "form-control input") %>
  </div>

  <div class="form-group">
    <%= form.label "Descripción de NC(solo si es necesario)" %>
    <%= form.text_area :description, class: "form-control input" %>
  </div>

  <div class="actions">
    <%= form.submit "Levantar No Conformidad", class: "btn btn-warning btn-block" %>
  </div>

<% end %>