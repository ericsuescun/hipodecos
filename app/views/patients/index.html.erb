<p id="notice"><%= notice %></p>

<h1>Patients</h1>

<table>
  <thead>
    <tr>
      <th>Id number</th>
      <th>Id type</th>
      <th>Birth date</th>
      <th>Age number</th>
      <th>Age type</th>
      <th>Name1</th>
      <th>Name2</th>
      <th>Lastname1</th>
      <th>Lastname2</th>
      <th>Sex</th>
      <th>Gender</th>
      <th>Address</th>
      <th>Email</th>
      <th>Tel</th>
      <th>Cel</th>
      <th>Occupation</th>
      <th>Residence code</th>
      <th>Municipality</th>
      <th>Department</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @patients.each do |patient| %>
      <tr>
        <td><%= patient.id_number %></td>
        <td><%= patient.id_type %></td>
        <td><%= patient.birth_date %></td>
        <td><%= patient.age_number %></td>
        <td><%= patient.age_type %></td>
        <td><%= patient.name1 %></td>
        <td><%= patient.name2 %></td>
        <td><%= patient.lastname1 %></td>
        <td><%= patient.lastname2 %></td>
        <td><%= patient.sex %></td>
        <td><%= patient.gender %></td>
        <td><%= patient.address %></td>
        <td><%= patient.email %></td>
        <td><%= patient.tel %></td>
        <td><%= patient.cel %></td>
        <td><%= patient.occupation %></td>
        <td><%= patient.residence_code %></td>
        <td><%= patient.municipality %></td>
        <td><%= patient.department %></td>
        <td><%= link_to 'Show', patient %></td>
        <td><%= link_to 'Edit', edit_patient_path(patient) %></td>
        <td><%= link_to 'Destroy', patient, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Patient', new_patient_path %>
