<p id="notice"><%= notice %></p>

<h1>Physicians</h1>

<table>
  <thead>
    <tr>
      <th>Inform</th>
      <th>User</th>
      <th>Name</th>
      <th>Lastname</th>
      <th>Tel</th>
      <th>Cel</th>
      <th>Email</th>
      <th>Study1</th>
      <th>Study2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @physicians.each do |physician| %>
      <tr>
        <td><%= physician.inform %></td>
        <td><%= physician.user_id %></td>
        <td><%= physician.name %></td>
        <td><%= physician.lastname %></td>
        <td><%= physician.tel %></td>
        <td><%= physician.cel %></td>
        <td><%= physician.email %></td>
        <td><%= physician.study1 %></td>
        <td><%= physician.study2 %></td>
        <td><%= link_to 'Show', physician %></td>
        <td><%= link_to 'Edit', edit_physician_path(physician) %></td>
        <td><%= link_to 'Destroy', physician, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Physician', new_physician_path %>
