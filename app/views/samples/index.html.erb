<p id="notice"><%= notice %></p>

<h1>Samples</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Id</th>
      <th>Inform Id</th>
      <th>User</th>
      <!-- <th>Name</th> -->
      <!-- <th>Description</th> -->
      <th>Recip.</th>
      <th>Organ</th>
      <th>Tag</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @samples.each do |sample| %>
      <tr>
        <td><%= sample.id %></td>
        <td><%= sample.inform %></td>
        <td><%= sample.user_id %></td>
        <!-- <td><%= sample.name %></td> -->
        <!-- <td><%= sample.description %></td> -->
        <td><%= sample.organ_code %></td>
        <td><%= sample.sample_tag %></td>
        <td><%= link_to 'Show', sample %></td>
        <td><%= link_to 'Edit', edit_sample_path(sample) %></td>
        <td><%= link_to 'Destroy', sample, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sample', new_sample_path %>
