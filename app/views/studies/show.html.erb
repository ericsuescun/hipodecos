<%= link_to 'Editar CUPS ' + Codeval.find(@study.codeval_id).code.to_s + " de informe " + take_inf.tag_code, edit_study_path(@study) %> |
<%= link_to 'Volver al informe', inform_path(@inf) %>

<div class="container-fluid">
  <div class="row">
    <div class="col-12 col-lg-4 offset-lg-2">
    	<div class="alert alert-success">
    	  <h1>CUPS</h1>
    	  <fieldset disabled>
    	  	<%= form_with(model: @study, local: true) do |form| %>

    	  	  <div class="form-group">
    	  	    <%= form.label "Código CUPS" %>
    	  	    <%= form.select :codeval_id, Codeval.pluck(:code, :id), {prompt:"Seleccione el CUPS"}, class: "form-control" %>
    	  	  </div>

    	  	  <div class="form-group">
    	  	    <%= form.label "Factor" %>
    	  	    <%= form.number_field :factor, class: "form-control input" %>
    	  	  </div>

    	  	<% end %>
    	  </fieldset>
    	</div>
    </div>
    <div class="col-12 col-lg-4">
      <div class="alert alert-warning">
        <h1>Revisión</h1>
        <%= render partial: "objections/form", locals: { objectionable: @study } %>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-12 col-lg-12">
	    <div class="alert alert-warning">
	    <%= render partial: "objections/objections_list", locals: { objectionable: @study } %>
	  	</div>
    </div>
  </div>
</div>