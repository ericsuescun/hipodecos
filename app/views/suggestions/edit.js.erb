$(document).on('page:load', refresh());

function refresh() {

	$('div.suggestions-out').remove();
	$('div.suggestions-in').append('<%= j render "suggestions/suggestions", inform: @inform, edit_suggestion: @suggestion, edit_objection_id: nil, edit_su_status: true %>');

	
	setTimeout(function() {
		$('div.alert-growl').remove();
	}, 3000)
}